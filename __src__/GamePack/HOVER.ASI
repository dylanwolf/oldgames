FOR COL = 22 TO 58
LOCATE ROW,COL
COLOR 0,1
PRINT " "
NEXT COL
NEXT ROW
GOSUB BFMOVE:

FASTER:
IF SPDSET = 0 THEN BFMOVE:
SPDSET = SPDSET - 5 
GOSUB BFMOVE:

SLOWER:
IF SPDSET = 5000 THEN BFMOVE:
SPDSET = SPDSET + 5 
GOSUB BFMOVE:

